{
  "name": "coloring-book-worker",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsup src/index.ts --format esm,cjs --dts --sourcemap --clean",
    "deploy": "prisma migrate deploy || echo 'Migration failed, continuing...'",
    "studio": "prisma studio",
    "railway:build": "prisma generate && npm run build",
    "railway:start": "prisma generate && npm run deploy && npm start"
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "@prisma/engines": "^5.20.0",
    "@upstash/redis": "^1.28.4",
    "express": "^4.19.2",
    "openai": "^4.61.0",
    "pino": "^9.5.0",
    "prisma": "^5.20.0",
    "replicate": "^0.33.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "tsx": "^4.19.2",
    "typescript": "^5.6.2"
  }
}